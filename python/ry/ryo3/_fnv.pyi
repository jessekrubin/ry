"""ryo3-fnv types"""

import typing as t

from ry._types import Buffer

class fnv1a:  # noqa: N801
    name: t.Literal["fnv1a"]
    digest_size: t.Literal[8]
    block_size: t.Literal[1]

    def __init__(self, input: Buffer | None = None, key: int | None = None) -> None: ...
    def update(self, input: Buffer) -> None: ...
    def digest(self) -> Buffer: ...
    def intdigest(self) -> int: ...
    def hexdigest(self) -> str: ...
    def copy(self) -> fnv1a: ...

def fnv(input: Buffer, key: int | None = None) -> fnv1a: ...
